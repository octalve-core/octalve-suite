{
  "name": "TeamMember",
  "type": "object",
  "properties": {
    "email": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "role": {
      "type": "string",
      "enum": ["pm", "designer", "developer", "strategist", "copywriter"]
    },
    "avatar_url": {
      "type": "string"
    },
    "active_phases_count": {
      "type": "number",
      "default": 0
    }
  },
  "required": ["email", "name", "role"]
}
